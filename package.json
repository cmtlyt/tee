{
  "name": "@cmtlyt/tee",
  "version": "0.5.1",
  "description": "支持 ts 的仿 egg 后端框架",
  "packageManager": "pnpm@10.2.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "tee": "bin/cli.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmtlyt/tee.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "type": "module",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "unbuild --watch",
    "build": "unbuild",
    "prepare": "husky",
    "lint": "eslint src",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "keywords": [
    "tee",
    "egg",
    "koa",
    "ts",
    "backend"
  ],
  "author": {
    "name": "cmtlyt",
    "email": "cmtlyt@163.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.24.0",
    "eslint-plugin-format": "^1.0.1",
    "esno": "^4.8.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "standard-changelog": "^6.0.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  },
  "dependencies": {
    "@cmtlyt/base": "^0.11.4",
    "@koa/router": "^13.1.0",
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.4",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "esbuild": "^0.25.2",
    "fast-glob": "^3.3.3",
    "jiti": "^2.4.2",
    "koa": "^2.16.1",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "rimraf": "^6.0.1",
    "scule": "^1.3.0",
    "untyped": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
